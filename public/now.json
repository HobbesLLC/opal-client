{
	"version": 2,
	"name": "opal",
	"routes": [
	   {
	     "src": "^/static/(.*)",
	     "dest": "/static/$1"
	   },
	   { "src": "^/favicon.ico$",
	     "dest": "/favicon.ico"
	   },
	   {
	     "src": "^/manifest.json$",
	     "dest": "/manifest.json"
	    },
	   {
	     "src": ".*",
	     "dest": "/index.html"
	   }

	],
	"env": {
	  "REACT_APP_API_TOKEN": "@react_app_api_token",
	  "REACT_APP_API_ENDPOINT": "@react_app_api_endpoint"
	}
 }
